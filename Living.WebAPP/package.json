{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && nuxt dev",
    "lint": "eslint . --ignore-pattern './api/schema.d.ts'",
    "lint:fix": "eslint --fix . --ignore-pattern './api/schema.d.ts'",
    "openapi": "NODE_TLS_REJECT_UNAUTHORIZED=0 openapi-typescript https://localhost:7129/swagger/v1/swagger.json  --enum -o ./api/schema.ts",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "clean": "rm -rf .nuxt dist .output",
    "clean:all": "npm run clean node_modules"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.14",
    "@nuxt/eslint": "^0.5.5",
    "@nuxtjs/i18n": "^8.5.0",
    "@primevue/nuxt-module": "^4.0.5",
    "@vueuse/core": "^11.0.3",
    "@vueuse/nuxt": "^11.0.3",
    "nuxt": "^3.13.0",
    "openapi-typescript": "^7.3.0",
    "typescript": "^5.5.4",
    "vue": "^3.5.5",
    "vue-router": "^4.4.3"
  },
  "dependencies": {
    "@primevue/themes": "^4.0.5",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.0.7",
    "quill": "^1.3.7"
  }
}
